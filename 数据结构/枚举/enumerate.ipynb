    "import enum\n",
    "\n",
    "\n",
    "@enum.unique\n",
    "class WeekDays(enum.IntEnum):\n",
    "    # 枚举常量列表\n",
    "    MONDAY = 1\n",
    "    TUESDAY = 2\n",
    "    WEDNESDAY = 3  # 'Wed.'\n",
    "    THURSDAY = 4\n",
    "    FRIDAY = 5  # 1\n",
    "\n",
    "\n",
    "day = WeekDays.FRIDAY\n",
    "\n",
    "if day == WeekDays.MONDAY:\n",
    "    print('工作')\n",
    "elif day == WeekDays.FRIDAY:\n",
    "    print('学习')"
   ]
